<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="smartgrid.newsimcontrol.rcp.SmartgridRCPApplication">
         </run>
      </application>
   </extension>
   <extension
         id="testProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Sirius.Product">
         <property
               name="appName"
               value="Sirius.Product">
         </property>
      </product>
   </extension>

</plugin>
